{
  "name": "worktree-backend",
  "version": "1.0.0",
  "description": "Backend for Worktree using Express and Prisma",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:ws": "tsx src/ws-server.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --force",
    "prisma:generate": "prisma generate",
    "seed": "tsx --env-file=.env src/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@hocuspocus/extension-database": "^3.4.3",
    "@hocuspocus/extension-logger": "^3.4.3",
    "@hocuspocus/server": "^3.4.3",
    "@hocuspocus/transformer": "^3.4.3",
    "@prisma/client": "^5.22.0",
    "@types/multer": "^2.0.0",
    "ai": "^6.0.41",
    "archiver": "^7.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lib0": "^0.2.117",
    "multer": "^2.0.2",
    "openai": "^6.16.0",
    "pdf-lib": "^1.17.1",
    "prisma": "^5.22.0",
    "rate-limit-redis": "^4.2.0",
    "bullmq": "^5.50.0",
    "redis": "^4.6.12",
    "sharp": "^0.33.5",
    "uuid": "^13.0.0",
    "y-protocols": "^1.0.7",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.29",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
