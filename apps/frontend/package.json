{
  "name": "worktree-forms-frontend",
  "version": "1.0.0",
  "description": "Frontend for Worktree-Forms using Next.js 14",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
